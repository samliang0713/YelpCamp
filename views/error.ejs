<% layout('layouts/boilerplate') %>
<div class="row">
  <div class="col-6 offset-3">
    <div class="alert alert-danger" role="alert">
      <% if(process.env.NODE_ENV !=="production" ) { %>
      <h4 class="alert-headring"><%= err.message%></h4>
      <hr />

      <p><%=err.stack%></p>

      <% } else { %>
      <h4 class="alert-headring">Oh no! Something went wrong...</h4>
      <% } %>
    </div>
  </div>
</div>
